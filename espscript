vol 25
tft.setup(0, 0, 0)
tft.bmp("/uploads/bild.jpg", 0, 0, 2220) 
tft.text.color(grey)
tft.text.font(2)
tft.text.size(2)
tft.text.cursor(0,50)
tft.println("Be patient. Agent is")
tft.println("comming up, soon.")

freq 146
RX
am
delay 3000
freq 3573
usb

port = 8080
print "UDP-Server "&ip()&":"&port
udpbegin port
udpbranch client
textbox clientmsg
wait

client:
tft.bmp("/uploads/bild.jpg", 0, 0, 2220) 
clientmsg = udpread()
tft.text.cursor(0,50)
tft.println("Agent orders butter on") 
tft.print(clientmsg) 
freq clientmsg
udpreply "OK"
return

